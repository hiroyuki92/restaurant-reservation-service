includes:
    - vendor/nunomaduro/larastan/extension.neon

parameters:
    level: 5
    paths:
        - app/
        - bootstrap/
        - config/
        - database/
        - public/
    ignoreErrors:
        - '#Call to an undefined method Illuminate\\.+#'
    checkMissingIterableValueType: false
    scanFiles:
        - bootstrap/app.php
